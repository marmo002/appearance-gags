<section>
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-md-10">

        <%#= render 'welcome_form/partials/welcome_submenu' %>
        <h2 class="custom-subtitle-1">Step 6 of 8: Company logo</h2>
        <p class="subtle_subtitle">Company logo you want to promote on the show.</p>

        <div class="progress mb-3">
          <div class="progress-bar" role="progressbar" style="width: 72%;" aria-valuenow="72" aria-valuemin="0" aria-valuemax="100"> Initial intake 72%</div>
        </div>
        <div class="tab-content" id="welcome-inputs-wrapper">
          <%= render 'dashboard/show_errors' %>
          <div class="tab-pane fade show active" id="company-info">
            <%= form_with model: current_user, url: welcome_update_path, html: { class: "customer-form", id: "user-profile-form", novalidate: true, data: { next: welcome_company_social_path, 'js-file-upload' => true } } do |f| %>
              <%= render 'welcome_form/partials/company_image_form', f: f %>
              <div class="row">
                <div class="col-md-12">
                  <%= link_to "Back", welcome_company_legal_path, class: "btn btn-primary"  %>
                  <%= f.submit "Next", class: "btn btn-primary float-right", role: "button", data: { "disable-with": "Updating..." } %>
                </div>
              </div>
            <% end %>
          </div><!--profile-tab-->
        </div>
      </div>
    </div>
  </div>
</section>
